<Window x:Class="WpfDemoApp.Layouts.WrapPanelLayoutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDemoApp.Layouts"
        mc:Ignorable="d"
        Title="WrapPanelLayoutWindow" Height="450" Width="800">
    <WrapPanel>
    <Button Height="100" Width="100" Template="{StaticResource EllipseButton}" Background="Green" >One</Button>
    <Button Height="100" Width="100">Two</Button>
    <Button Height="100" Width="100">Three</Button>
    <Button Height="100" Width="100">Four</Button>
    <Button Height="100" Width="100">Five</Button>
  </WrapPanel>
</Window>
